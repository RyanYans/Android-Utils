<?xml version="1.0" encoding="utf-8"?>
<ScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content">

    <view.FlowLayout_Zone
        android:id="@+id/flowLayoutZone1"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">

        <Button
            android:id="@+id/handle"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_margin="4dp"
            android:onClick="onClick"
            android:text="handler测试" />

        <TextView
            android:id="@+id/tv_okHttp"
            android:layout_width="match_parent"
            android:layout_height="30dp"
            android:background="#00ff00"
            android:gravity="center"
            android:text="OKhttp学习" />

        <Button
            android:id="@+id/bt_okGet"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_margin="4dp"
            android:onClick="onClick"
            android:text="get" />

        <Button
            android:id="@+id/bt_okPost"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_margin="4dp"
            android:onClick="onClick"
            android:text="post" />

        <Button
            android:id="@+id/bt_okUpload"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_margin="4dp"
            android:onClick="onClick"
            android:text="文件上传" />

        <Button
            android:id="@+id/bt_Https"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_margin="4dp"
            android:onClick="onClick"
            android:text="12306Https" />

        <Button
            android:id="@+id/bt_downLoader"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_margin="4dp"
            android:onClick="onClick"
            android:text="ok下载" />

        <Button
            android:id="@+id/bt_downLoaderpause"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_margin="4dp"
            android:onClick="onClick"
            android:text="ok下载 pause" />

        <Button
            android:id="@+id/bt_downLoaderDelete"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_margin="4dp"
            android:onClick="onClick"
            android:text="删除下载" />

        <ProgressBar
            android:id="@+id/progressBar"
            style="?android:attr/progressBarStyleHorizontal"
            android:layout_width="match_parent"
            android:layout_height="10dp"
            android:max="100"
            android:progress="0" />

        <TextView
            android:layout_width="match_parent"
            android:layout_height="30dp"
            android:background="#00ff00"
            android:gravity="center"
            android:text="Thread测试" />

        <Button
            android:id="@+id/asyncTask"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_margin="4dp"
            android:onClick="onClick"
            android:text="AsyncTask测试" />

        <TextView
            android:layout_width="match_parent"
            android:layout_height="30dp"
            android:background="#00ff00"
            android:gravity="center"
            android:text="Retrofit2.0Sample" />

        <Button
            android:id="@+id/retrofitGet"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_margin="4dp"
            android:onClick="onClick"
            android:text="retrofitGet" />
        <Button
            android:id="@+id/retrofitPost"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_margin="4dp"
            android:onClick="onClick"
            android:text="retrofitPost" />
        <Button
            android:id="@+id/retrofitFile"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_margin="4dp"
            android:onClick="onClick"
            android:text="retrofitFile" />

        <TextView
            android:layout_width="match_parent"
            android:layout_height="30dp"
            android:background="#00ff00"
            android:gravity="center"
            android:text="封装NetworkPull测试" />

        <Button
            android:id="@+id/noPullTest"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_margin="4dp"
            android:onClick="onClick"
            android:text="noPull测试" />

        <Button
            android:id="@+id/noPull_GloboTest"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_margin="4dp"
            android:onClick="onClick"
            android:text="noPull_Globlo测试" />

        <Button
            android:id="@+id/pullGet"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_margin="4dp"
            android:onClick="onClick"
            android:text="havePull_Get测试" />

    </view.FlowLayout_Zone>
</ScrollView>

